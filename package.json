{
  "name": "bloombot",
  "version": "1.0.0",
  "description": "Bot designed for the Meditation Mind Discord.",
  "main": "node bot.js",
  "engines": {
    "node": "^16.9.1"
  },
  "author": {
    "name": "Zachary Montgomery",
    "url": "https://codingprojects.org/"
  },
  "scripts": {
    "build": "rm -rf dist && tsc",
    "start": "tsc && node dist/bot.js",
